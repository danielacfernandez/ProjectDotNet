        <displayRowCursor>True</displayRowCursor>
        <sqlSelect><![CDATA[SELECT
	MRCC.ID AS CategoryID,
	MRCC.TableID AS CategoryTableID,
	MRC.EndUserID as EndUserID,
	MRCC.Name,
	MRCC.Sequence,
	MRCC.Quantity,
	MRCC.Dollars,
	MRCC.Average,
	MRCC.RateCalculation,
	CASE MRCC.RateCalculation 
		WHEN 'GP' THEN 'GP' 
		WHEN 'ActivityType' THEN 'Activity Type'
		WHEN 'WorkCode' THEN 'Work Code'
		ELSE NULL 
	END AS RateCalculationName,
	MRCC.IsActive

FROM MAC_Category_Config MRCC (nolock)
INNER JOIN MAC_Table_Config MTC (nolock)
	ON MTC.ID = MRCC.TableID
INNER JOIN MAC_Report_Config MRC (nolock)
	ON MTC.ReportID = MRC.ID
WHERE MRCC.TableID = @TableID]]>
<parameters /></sqlSelect>
        <sqlUpdate where="[CategoryID]=@CategoryID" autoUpdateQuery="False" updateTable=""><![CDATA[UPDATE MAC_Category_Config SET
	TableID = @CategoryTableID,
	Name = @Name,
	Sequence = @Sequence,
	Quantity = @Quantity,
	Dollars = @Dollars,
	Average = @Average,
	RateCalculation = @RateCalculation,
	ModifiedOn = GETUTCDATE(),
	ModifiedBy = @__UserId__,
	IsActive = @IsActive
WHERE ID=@CategoryID]]></sqlUpdate>
        <sqlInsert where="[CategoryID]=@@IDENTITY"><![CDATA[INSERT INTO MAC_Category_Config (TableID, Name, Sequence, Quantity, Dollars, Average, RateCalculation, CreatedOn, CreatedBy, IsActive) VALUES (
	@TableID,
	@Name,
	@Sequence,
	@Quantity,
	@Dollars,
	@Average,
	@RateCalculation,
	GETUTCDATE(),
	@__UserId__,
	@IsActive
)]]></sqlInsert>
        <sqlDelete><![CDATA[DELETE FROM MAC_Category_Config
WHERE ID=@CategoryID]]></sqlDelete>
