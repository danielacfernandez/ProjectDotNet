        <sqlSelect><![CDATA[select 
	U.AspNetUsersCustomersId,
	U.CustomerId, 
	C.CUSTOMER_NAME,
	U.CreateTime, 
	U.CreatedBy, 
	U.IsActive, 
	U.ModifyTime, 
	U.ModifiedBy
from
	dbo.AspNetUserCustomers U
left join
	dbo.Customers C
on
	U.CustomerID=C.CUSTOMER_ID
where
	U.UserID=@USERID]]>
<parameters /></sqlSelect>
        <sqlUpdate where="1=1" autoUpdateQuery="False" updateTable=""><![CDATA[-- ignore the insert if the USERID/ORGANIZATIONID pair already exists
if not exists(select top 1 * from [dbo].[AspNetUserCustomers] where UserId=@USERID and CustomerID=@CUSTOMERID and AspNetUsersCustomersId<>@ASPNETUSERSCUSTOMERSID)	
begin
	update 
		[dbo].[AspNetUserCustomers]
	set
		CustomerID=@CUSTOMERID,
		IsActive=isnull(@ISACTIVE, 0),
		ModifyTime=getutcdate(),
		ModifiedBy=@RUNNING_AS_USERID
	where	
		AspNetUsersCustomersId=@ASPNETUSERSCUSTOMERSID
end]]></sqlUpdate>
        <sqlInsert where="[AspNetUsersCustomersId]=@@IDENTITY"><![CDATA[-- ignore the insert if the USERID/CUSTOMERID pair already exists
if not exists(select top 1 * from [dbo].[AspNetUserCustomers] where UserId=@USERID and CustomerID=@CUSTOMERID)	
begin
	insert into
		[dbo].[AspNetUserCustomers] (UserId, CustomerID, IsActive, CreateTime, CreatedBy)
	values
		(@USERID, @CUSTOMERID, isnull(@ISACTIVE, 1), getutcdate(), @RUNNING_AS_USERID)
end]]></sqlInsert>
        <sqlDelete><![CDATA[delete from [dbo].[AspNetUserCustomers] where AspNetUsersCustomersId=@ASPNETUSERSCUSTOMERSID]]></sqlDelete>
