        <sqlSelect><![CDATA[select 
	C.*,
	UC.FullName as CreatedByUser,
	UM.FullName as ModifiedByUser,
	L.NAME as CustomerType
from 
	dbo.Customers C
left join
	dbo.AspNetUsers UC
on
	C.CREATED_BY = UC.UserId
left join
	dbo.AspNetUsers UM
on
	C.MODIFIED_BY = UM.UserId
left join
	dbo.LOOKUPS L
on
	L.LOOKUP_ID = C.customer_type_id


]]>
