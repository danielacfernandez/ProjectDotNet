	CREATED_BY_NAME, 
	INVOICE_TYPE_NAME, 
	BILLING_TYPE_NAME, 
	ASSIGNED_TO_NAME, 
	QUOTETOTAL, 
	PO_NO, 
	dbo.udf_StripHTML(DESCRIPTION) AS DESCRIPTION, 
	TrackingNotes,
	JOB_NO,
	CUSTOMER_NAME, 
	ProjectManagerName, 
	JOB_NAME, 
	EndUserName, 
	INV_AMTTOBILL, 
	EXPENSESTOTAL, 
	DATE_CREATED, 
	DATE_SENT,
	BillingPeriod
FROM [dbo].[Invoicing_GetInvoicesOpen_FN](
	@ORGANIZATION_ID, 
	@RUNNING_AS_USERID, 
	@FROMDATE, 
	@TODATE, 
	@JOBID
)]]>
<parameters /></sqlSelect>
