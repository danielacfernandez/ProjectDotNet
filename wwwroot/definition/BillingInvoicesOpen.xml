        <sqlSelect>
		<![CDATA[SELECT 
	'<a href="#" onclick="openJobInvoice('+cast( JOB_ID as varchar)+', '+cast( INVOICE_ID as varchar)+', ''UNBILLEDOPS'')">' + cast( INVOICE_ID as varchar) + '</a>' as INVOICE_ID_LINK, 
