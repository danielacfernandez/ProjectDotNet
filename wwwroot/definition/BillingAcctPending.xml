        <sqlSelect><![CDATA[SELECT 
	'<a href="#" onclick="openJobInvoice('+cast( @JOBID as varchar)+', '+cast( INVOICE_ID as varchar)+', ''UNBILLEDACCT'')">' + cast( INVOICE_ID as varchar) + '</a>' as INVOICE_ID_LINK,
