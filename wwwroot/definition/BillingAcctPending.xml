	JOB_NO, 
	PO_NO, 
	CUSTOMER_NAME, 
	dbo.udf_StripHTML(invoice_description) AS invoice_description, 
	HOURSTOTAL, 
	EXPTOTAL, 
	CUSTOMTOTAL, 
	TOTAL, 
	dealer_name, 
	job_type_name, 
	assigned_to_name, 
	DATECREATED, 
	BATCHID, 
	batch_status_name, 
	@JOBID as JOB_ID, 
	InvoiceTotal, 
	Date_Sent, 
	BILLING_TYPE_NAME,
	TrackingNotes,
	BillingPeriod
FROM [dbo].[Invoicing_GetInvoices_FN](
	@ORGANIZATION_ID,  
	@RUNNING_AS_USERID, 
	@JOBID,
       'PENDING'
)]]>
