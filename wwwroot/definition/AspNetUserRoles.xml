        <sqlSelect><![CDATA[select 
	U.UserRolesID, 
	U.UserID AS RoleUserID, 
	U.RoleID, 
	R.Name as RoleName,
	U.CreatedDT, 
	U.IsActive, 
	U.ModifyTime, 
	U.ModifiedBy
from
	[dbo].[AspNetUserRoles] U
left join
	[dbo].[AspNetRoles] R
on
	U.RoleID=R.RoleID
where
	UserID=@USERID]]>
<parameters /></sqlSelect>
        <sqlUpdate where="UserRolesID=@USERROLESID" autoUpdateQuery="False" updateTable=""><![CDATA[-- ignore the insert if the USERID/ROLEID pair already exists
if not exists(select top 1 * from [dbo].[AspNetUserRoles] where UserId=@USERID and RoleID=@ROLEID and UserRolesID<>@USERROLESID)	
begin
	update 
		[dbo].[AspNetUserRoles]
	set
		RoleID=@ROLEID,
		--IsActive=@ISACTIVE,
		ModifyTime=getutcdate(),
		ModifiedBy=@RUNNING_AS_USERID
	where	
		UserRolesID=@USERROLESID
end]]></sqlUpdate>
        <sqlInsert where="[UserRolesID]=@@IDENTITY"><![CDATA[-- ignore the insert if the USERID/ROLEID pair already exists
if not exists(select top 1 * from [dbo].[AspNetUserRoles] where UserId=@USERID and RoleID=@ROLEID)	
begin
	insert into
		[dbo].[AspNetUserRoles] (UserId, RoleID, CreatedDT, IsActive, ModifyTime, ModifiedBy)
	values
		(@USERID, @ROLEID, getutcdate(), 1, getutcdate(), @RUNNING_AS_USERID)
end]]></sqlInsert>
        <sqlDelete><![CDATA[delete from [dbo].[AspNetUserRoles] where UserRolesID=@USERROLESID]]></sqlDelete>
