        <sqlSelect><![CDATA[select 
	   JL.[JOB_LOCATION_ID]  
      ,JL.[JOB_LOCATION_NAME]  
	  ,JL.[CUSTOMER_ID]  
      ,JL.[loading_dock_type_ID]  
	  ,[dbo].[getLookupName](JL.[loading_dock_type_ID], null, null )  as [loading_dock_type_name]
      ,JL.[DOCK_HEIGHT]  
      ,JL.[DOCK_RESERV_REQ_TYPE_ID]
	  ,L1.LOOKUP_NAME  as [DOCK_RESERV_REQ_TYPE_NAME]
      ,JL.[ELEVATOR_AVAIL_TYPE_ID]  
	  ,L2.LOOKUP_NAME  as [ELEVATOR_AVAIL_TYPE_NAME]
      ,JL.[FLOOR_PROTECTION_TYPE_ID]
	  ,L3.LOOKUP_NAME  as [FLOOR_PROTECTION_TYPE_NAME]
      ,JL.[WALL_PROTECTION_TYPE_ID]
	  ,L4.LOOKUP_NAME as [WALL_PROTECTION_TYPE_NAME]
      ,JL.[DOORWAY_PROT_TYPE_ID]
	  ,L5.LOOKUP_NAME  as [DOORWAY_PROT_TYPE_NAME]
      --,JL.[LOCATION_TYPE_ID]  
      --,JL.[location_type_code]  
      --,JL.[location_type_name]  
      --,JL.[EXT_ADDRESS_ID]  
      ,JL.[STREET1]  
      ,JL.[STREET2]  
      ,JL.[STREET3]  
      ,JL.[CITY]  
      ,JL.[STATE]  
      ,JL.[ZIP]  
      --,JL.[COUNTRY]  
      --,JL.[DIRECTIONS]   
from 
	dbo.job_locations_v JL
left join
	dbo.Lookups_Active_V L1
on
	jl.[loading_dock_type_ID]=L1.LOOKUP_ID
left join
	dbo.Lookups_Active_V L2
on
	jl.[ELEVATOR_AVAIL_TYPE_ID]=L2.LOOKUP_ID
left join
	dbo.Lookups_Active_V L3
on
	jl.[FLOOR_PROTECTION_TYPE_ID]=L3.LOOKUP_ID
left join
	dbo.Lookups_Active_V L4
on
	jl.[WALL_PROTECTION_TYPE_ID]=L4.LOOKUP_ID
left join
	dbo.Lookups_Active_V L5
on
	jl.[DOORWAY_PROT_TYPE_ID]=L5.LOOKUP_ID
where 
	ORGANIZATION_ID = @ORGANIZATION_ID]]>
