        <sqlSelect><![CDATA[select 
[QuoteId]
      ,[QuoteLineDayId]
      --,convert(varchar(10),[Date],110) as 
	  ,[Date]
      ,[StartTime]
      ,[OnSiteTime]
      ,[day]
      ,[Description]
      ,[HoursReg]
      ,[HoursOT]
      ,[QuoteLineDayIdCUSTOMTU]
      ,[QuoteLineDayIDINSTALL]
      ,[QuoteLineDayIDPM]
      ,[QuoteLineDayIDSUP]
      ,[CUSTOMTU]
      ,[INSTALL]
      ,[PM]
      ,[SUP]

, P.Date + StartTime as StartDateTimeToSort
,L.LOOKUP_NAME as StartTimeName 
,LO.LOOKUP_NAME as OnSiteTimeName 
		from 
dbo.[Quote_OperationalPlan_ICS_V] P
left join [dbo].[GetLooukpValuesByType] ('time_list_15min' ) L
on L.Lookup_Code = P.StartTime
left join [dbo].[GetLooukpValuesByType] ('time_list_15min' ) LO
on LO.Lookup_Code = P.OnSiteTime
where P.QuoteId = @QUOTE_ID]]>
