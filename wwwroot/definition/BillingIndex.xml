        <sqlSelect><![CDATA[SELECT  
	'<a href="#" onclick="openJobBilling('+cast( JOB_ID as varchar)+')">' + cast( JOB_ID as varchar) + '</a>' as JOB_ID,
	'<a href="#" onclick="openJobBilling('+cast( JOB_ID as varchar)+')">' + cast(JOB_NO as varchar) + '</a>' as JOB_NO, 
	customer_name, 
	end_user_name, 
	JOB_NAME, 
	INVTOTAL, 
	BILLABLETOTAL + POItemTotal as BILLABLETOTAL, 
	NONBILLABLETOTAL, 
	UnapprovedCost,
	ProjectManagerName, 
	MaxSREndDate, 
	MAXSLDateCreated,
	MAXSLTEEndDate,
	sales_contact_name,
	po_no,
        POItemTotal,
        POOpenedCount,  
        POInvoicedCount,  
        POClosedCount
FROM dbo.Invoicing_GetBillingOverview_FN_LSP(@ORGANIZATION_ID, @__USERID__, @USERTYPE)
WHERE BILLABLETOTAL > 0 /*OR UnapprovedCost > 0*/
or POItemTotal > 0]]>
