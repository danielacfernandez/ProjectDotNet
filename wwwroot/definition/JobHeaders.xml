        <sqlSelect><![CDATA[select 
 J.JOB_ID,
 J.JOB_NO, 
 J.JOB_NAME, 
 J.Customer_Id,
 J.customer_name, 
 J.end_user_id,
 J.end_user_name,
 J.JOB_TYPE_ID,
 J.job_type_name,
 J.job_status_type_name, 
 J.IsCustomNumber,
 J.PriceLevel,
 J.PriceLevel as PriceLevelName,
 J.ExtDealerId,
 @ORGANIZATION_ID as ORGANIZATIONID,
 Request_Id
from
dbo.JobHeaders_FN(@ORGANIZATION_ID, 
@__LoginId__
) J]]>
<parameters /></sqlSelect>
        <sqlUpdate where="[JOB_ID]=@JOB_ID" autoUpdateQuery="False" updateTable=""><![CDATA[declare @useridval bigint
select @useridval=UserId from dbo.AspNetUsers where username = @__LoginId__;
exec dbo.Job_Update
  @JobId=@JOB_Id,
  @JobName=@JOB_NAME,
  @PriceLevel=@PRICELEVEL,
  @UserId=@useridval]]></sqlUpdate>
        <sqlInsert where=""><![CDATA[declare @useridval bigint
select @useridval=UserId from dbo.AspNetUsers where username = @__LoginId__;
exec [dbo].[Job_CreateHeader]
 @ProjectNo = @JOB_NO
,@JobTypeID = @JOB_TYPE_ID
,@CustomerID = @CUSTOMER_ID
,@EndUserID = @END_USER_ID
,@JobName = @JOB_NAME
,@UserId = @useridval]]></sqlInsert>
