        <sqlSelect><![CDATA[select 
	BILL_SERVICE_NO, 
	PO_NO, 
	BILLING_TYPE_NAME, 
	dbo.udf_StripHTML(SERVICE_DESCRIPTION) AS SERVICE_DESCRIPTION, 
	ITEM_NAME, 
	SUM_PAYROLL_QTY, 
	SUM_BILL_HOURLY_QTY, 
	SUM_BILL_EXP_QTY, 
	BILL_RATE, 
	BILL_JOB_NO,
	BILL_HOURLY_TOTAL, 
	BILL_EXP_TOTAL, 
	BILL_TOTAL, 
	QUOTED_TOTAL, 
	ITEM_ID, 
	BILL_SERVICE_ID, 
	CAST(ITEM_ID AS VARCHAR(25)) + '-' + CAST(BILL_SERVICE_ID AS VARCHAR(25)) AS KEYID,
	'<a href="#" onclick="openRequest('+cast(REQUEST_ID as varchar)+')">' + cast( BILL_JOB_NO as varchar) + '</a>' as JOB_REQUEST_LINK,
	'<a href="#" onclick="openRequest('+cast(LASTREQUEST_ID as varchar)+')">' + cast( BILL_SERVICE_NO as varchar) + '</a>' as JOB_LASTREQUEST_LINK	,
	Request_Name
from 
	dbo.Invoicing_GetBillingLinesByReqItem_FN(@JOBID,NULL,@ISBILLABLE,(SELECT USERID FROM DBO.AspNetUsers WHERE USERNAME = @__LoginId__),NULL)]]>
<parameters /></sqlSelect>
