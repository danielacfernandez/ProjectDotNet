        <buttons>
<button id="cmdInvoice" actionType="executeSQL" gridToOpen="" icon="fa fa-usd" title="Assign to Invoice" label="Assign to Invoice" target="" sqlParameters="KEYID" confirmationMessage="" multiselect="True" refreshDataAfterExecution="True" url="">
<sqlSentence><![CDATA[
					declare @VARCHARTable as VARCHARIDTABLETYPE
					insert into @VARCHARTable (ID) select cast(KEYID as varchar) from @KEYID
					EXEC [Billing_InvoiceServiceLines_Update]  @VARCHARTable, 'REQITEM', @INVOICEID, @__LoginId__ 
				]]></sqlSentence></button>
<button id="cmdBillable" actionType="executeSQL" gridToOpen="" icon="fa fa-check-square" title="Toggle Billability" label="Toggle Billability" target="" sqlParameters="KEYID" confirmationMessage="" multiselect="True" refreshDataAfterExecution="True" url="">
<sqlSentence><![CDATA[
					declare @VARCHARTable as VARCHARIDTABLETYPE
					DECLARE @V_ISBILLABLE VARCHAR(5)
					SET @V_ISBILLABLE = (SELECT CASE WHEN @ISBILLABLE = 'Y' THEN 'N' ELSE 'Y' END)
					insert into @VARCHARTable (ID) select cast(KEYID as varchar) from @KEYID 	
					EXEC [Billing_ServiceLinesBillable_Update]  @VARCHARTable, 'REQITEM', @V_ISBILLABLE, @__LoginId__ 
				]]></sqlSentence></button>
