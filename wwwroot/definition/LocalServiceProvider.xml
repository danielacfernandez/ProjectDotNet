        <sqlSelect><![CDATA[SELECT LSP.[LspID]
      ,LSP.[LspName]
      ,LSP.[Phone]
      ,LSP.[Address1]
      ,LSP.[Address2]
      ,LSP.[City]
      ,LSP.[State]
      ,LSP.[Zip]
      ,LSP.[Country]
      ,LSP.[CreatedDate]
      ,C.UserName AS [CreatedBy]
      ,LSP.[ModifiedDate]
      ,M.UserName AS [ModifiedBy]
      ,LSP.[IsActive]
      ,@ORGANIZATION_ID AS ORGANIZATION_ID
FROM [dbo].[LocalServiceProvider] LSP
LEFT JOIN AspNetUsers C
ON LSP.CreatedBy = C.UserId
LEFT JOIN AspNetUsers M
ON LSP.ModifiedBy = M.UserId]]>
<parameters /></sqlSelect>
        <sqlUpdate where="LspId = @LspId" autoUpdateQuery="False" updateTable=""><![CDATA[EXEC [dbo].[LocalServiceProvider_Update] 
	@LspId,
	@LspName,
	@Phone,
	@Address1,
	@Address2,
	@City,
	@State,
	@Zip,
	@Country,
	@USER_ID,
	@IsActive]]></sqlUpdate>
        <sqlInsert where="[LspId] = @@IDENTITY"><![CDATA[EXEC [dbo].[LocalServiceProvider_Insert]
	@LspName,
	@Phone,
	@Address1,
	@Address2,
	@City,
