        <sqlSelect><![CDATA[

SELECT [INVOICE_LINE_ID]
      ,[INVOICE_ID]
      ,IL.[ITEM_ID]
      ,[INVOICE_LINE_NO]
      ,[INVOICE_LINE_TYPE_ID]
      ,IL.[DESCRIPTION]
      ,[UNIT_PRICE]
      ,[QTY]
      ,[EXTENDED_PRICE]
      ,[PO_NO]
      ,[TAXABLE_FLAG]
      ,IL.[DATE_CREATED]
      ,IL.[CREATED_BY]
      ,IL.[DATE_MODIFIED]
      ,IL.[MODIFIED_BY]
      ,[bill_service_id]
	  ,UC.USERNAME AS CREATED_BY_USERNAME, UM.USERNAME AS MODIFIED_BY_USERNAME
	  ,I.NAME AS ITEM_NAME, L.NAME AS LINE_TYPE
  FROM [dbo].[INVOICE_LINES] IL
  LEFT JOIN DBO.ASPNETUSERS UC
	ON IL.CREATED_BY = UC.USERID
  LEFT JOIN DBO.ASPNETUSERS UM
	ON IL.MODIFIED_BY = UM.USERID
  LEFT JOIN DBO.ITEMS I
	ON IL.ITEM_ID = I.ITEM_ID
  LEFT JOIN DBO.LOOKUPS L
	ON IL.INVOICE_LINE_TYPE_ID = L.LOOKUP_ID
  WHERE INVOICE_ID = @INVOICE_ID
	
]]>
