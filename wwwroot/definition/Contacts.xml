        <sqlDelete><![CDATA[update 
  dbo.Contacts 
set 
  CONT_STATUS_TYPE_ID = [dbo].[getLookupId]('inactive', 'contact_status_type')
where 
  CONTACT_ID=@CONTACT_ID]]></sqlDelete>
