        <sqlSelect><![CDATA[select 
	C.[CONTACT_ID],
	C.[CONTACT_NAME],
	C.[ORGANIZATION_ID],
	C.[EXT_DEALER_ID],
	C.[CUSTOMER_ID],
	CU.CUSTOMER_NAME,
	C.[CONTACT_TYPE_ID],
	L1.LOOKUP_NAME AS [CONTACT_TYPE_NAME],
	C.[CONT_STATUS_TYPE_ID],
	L2.LOOKUP_NAME as [CONT_STATUS_TYPE_NAME],
	C.[PHONE_WORK],
	C.[PHONE_CELL],
	C.[PHONE_HOME],
	C.[EMAIL],
	C.[EMAIL_PHONE],
	C.[STREET1],
	C.[STREET2],
	C.[STREET3],
	C.[CITY],
	C.[STATE],
	C.[ZIP],
	C.[NOTES],
	C.[DATE_CREATED],
	C.[CREATED_BY],
	C.[DATE_MODIFIED],
	C.[MODIFIED_BY],
	C.[contact_id_8],
	C.[ExtEmployeeID],
	C.[ExternalContactID],
	@USER_ID as RUNNING_AS_USERID,
        CASE WHEN exists (select 1 from dbo.AspNetUsers with(nolock) where Contact_Id = C.Contact_Id) THEN 'Y' ELSE 'N' END UserContact
from 
	dbo.Contacts C
left join
	dbo.Lookups_Active_V L1
on
	L1.LOOKUP_ID = C.CONTACT_TYPE_ID
left join
	dbo.Lookups_Active_V L2
on
	L2.LOOKUP_ID = C.CONT_STATUS_TYPE_ID
left join
	dbo.Customers CU
on
	C.CUSTOMER_ID = CU.CUSTOMER_ID
and
	CU.ORGANIZATION_ID = @ORGANIZATION_ID
where
	C.ORGANIZATION_ID = @ORGANIZATION_ID]]>
<parameters /></sqlSelect>
        <sqlUpdate where="[CONTACT_ID]=@CONTACT_ID" autoUpdateQuery="False" updateTable=""><![CDATA[update 
	dbo.Contacts
set	
	[CONTACT_NAME]=@CONTACT_NAME,
	[EXT_DEALER_ID]=@EXT_DEALER_ID,
	[CONTACT_TYPE_ID]=@CONTACT_TYPE_ID,
	[CONT_STATUS_TYPE_ID]=@CONT_STATUS_TYPE_ID,
	[PHONE_WORK]=@PHONE_WORK,
	[PHONE_CELL]=@PHONE_CELL,
	[PHONE_HOME]=@PHONE_HOME,
	[EMAIL]=@EMAIL,
	[EMAIL_PHONE]=@EMAIL_PHONE,
	[STREET1]=@STREET1,
	[STREET2]=@STREET2,
	[STREET3]=@STREET3,
	[CITY]=@CITY,
	[STATE]=@STATE,
	[ZIP]=@ZIP,
	[NOTES]=@NOTES,
	[DATE_MODIFIED] = getutcdate(),
	[MODIFIED_BY] = @USER_ID,
	[ExtEmployeeID]=@ExtEmployeeID,
	[ExternalContactID]=@ExternalContactID
where
	CONTACT_ID = @CONTACT_ID]]></sqlUpdate>
