</div>

<!----------- Contact modal ------------->

<div class="modal fade modal--forms" data-bind="showModal:showContactModal" id="ContactModal" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="ContactLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="margin: 10rem auto;">
        <div class="modal-content modal-content--dark-blue">
            <div class="modal-header">
                <h5 class="modal-title mr-auto" id="ContactLabel">New Contact</h5>
